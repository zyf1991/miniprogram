/*-- 背景图 + 信息 start --*/
.my_top{
  display:flex;
  align-items:center;
  width:100%;
  height:450rpx;
  padding-left:40rpx;
  box-sizing:border-box;
  background: url(data:image/jpeg;base64,) center 50% no-repeat;
  background-size:100% 500rpx;
}
.my_topPic image{
  width:80px;
  height:80px;
  border-radius:50%;
}
.my_topName{
  margin-left:40rpx;
}
.topName{
  color:#fff;
  font-size:18px;
}
/*-- 背景图 + 信息 end --*/