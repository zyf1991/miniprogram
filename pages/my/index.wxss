/* pages/my/index.wxss */

/*-- 背景图 + 信息 start --*/
.my_top{
  display:flex;
  align-items:center;
  width:100%;
  height:450rpx;
  padding-left:40rpx;
  box-sizing:border-box;
  background: url(data:image/jpeg;base64,) center 50% no-repeat;
  background-size:100% 500rpx;
}
.my_topPic image{
  width:80px;
  height:80px;
  border-radius:50%;
}
.my_topName{
  margin-left:40rpx;
}
.topName{
  color:#fff;
  font-size:18px;
}
.topMyZYM{
  font-size:14px;
  font-weight:700;
}
/*-- 背景图 + 信息 end --*/
/*-- 目标+收藏+中心 start --*/
.mbsczx{
  transform: translateY(-10px);
  display: flex;
  align-items:center;
  justify-content:space-between;
  padding:10px 50rpx;
  box-sizing:border-box;
  border-radius:8px 8px 0px 0px;
  background:#fff;
}
.bangzhuBTN{
  position:unset;
  display:unset;
  border:0px!important;
  margin:unset;
  padding:unset;
  box-sizing:unset;
  font-size:unset;
  text-align:unset;
  color:unset;
  font-weight:unset;
  line-height:unset;
  background:unset;
}
.bangzhuBTN::after{
  position: unset;
  border:0px!important;
  width:unset;
  height:unset;
}
/*-- 目标+收藏+中心 end --*/
/*-- 我的空间 start --*/
.myZoom{
  display:flex;
  align-items:center;
  justify-content:space-between;
  padding:30rpx 40rpx 30rpx 30rpx;
  box-sizing:border-box;
}
/*-- 我的空间 end --*/
/*-- 投诉建议 start --*/
.tsjY{
  display:flex;
  align-items:center;
  justify-content:space-between;
  padding-left:30rpx;
}
/*-- 投诉建议 end --*/